{
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "testMatch": [
      "**/test/**/*.js?(x)"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  
  "dependencies": {
    
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "2.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "./node_modules/.bin/eslint  src/** test/** ",
    "test": "jest",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open"
  },
  "devDependencies": {
    "babel-jest": "^22.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cypress": "^2.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.3.0",
    "react-test-renderer": "^16.2.0",
    "chromedriver": "^2.35.0",
    "eslint": "^4.18.1",
    "jest-cli": "^22.3.0",
    "selenium-webdriver": "^4.0.0-alpha.1"
  }
}
